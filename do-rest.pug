extends templates/layout.pug

block vars
  - var activepage = "muatan";
  - var headerText = "SHIPPER";

block content

  .loading-page
    .loading-page__wrap
      .loading-page__logo
        .icon
          img(src="images/logo-icon.png")
        .text
          img(src="images/logo-text.png")
    span.foot-text PT ANTAR GLOBAL PROSPERO
  .block-dashboard
    section.inner-header
      .container
        .row
          .col-lg-7.animate.animate--up
            a.arrow.mobile(href="dashboard.html")
            h1 Delivery Order Sisa
          .col-lg-5.animate.animate--up
            .search
              form
                .form-group
                  input.form-control(type="text" placeholder="Ketik Nomor Purchase Order / Delivery Order ...")
        .breadcrubms.animate.animate--up
          u NAVIGATION :
          a(href="#") Homepage
          a(href="#") Dashboard
          a(href="#") PO Aktif
          span RINGKASAN PURCHASE ORDER
    section.your-do
      .container
        .your-do__do-det.do-rest.animate.animate--up
          .row
            .col-lg-6
              .box-po.animate.animate--up
                span.small-title Nomor Purchase Order
                h3 AGR001/02/2020#096
                .progress-bar
                  .pb
                    .pb-block
                      span(style="width: 20%")
                    .prcntg 20%
                  small Prosentase pengiriman muatan Anda.
                .row
                  .col-lg-6
                    .box-text
                      p Batubara
                      small Nama Barang
                  .col-lg-6
                    .box-text
                      p 20.000 TON
                      small Tonase / Kubikasi
            .col-lg-3
              .box-card.light-green.animate.animate--up
                .icon.hide-mobile
                  .ic(style="background-image: url(images/ic-rect-do-active.png)")
                h4 <b>DO</b> Aktif
                .text
                  p Delivery order yang sedang dalam pengiriman.
                .total
                  span 400
                .link-seemore
                  a(href="#") Lihat lebih lanjut
            .col-lg-3
              .box-card.cream.animate.animate--up
                .icon.hide-mobile
                  .ic(style="background-image: url(images/ic-rect-do-sisa.png)")
                h4 <b>PO</b> Sisa
                .text
                  p Delivery order yang sedang kami proses.
                .total
                  span 200
                .link-seemore
                  a(href="#") Lihat lebih lanjut
          a.ext-link(href="#" target="_blank")

    section.po-and-features
      .container
        .row
          .col-lg-2
            .box-card.small.sent.bg-white.animate.animate--up
              .icon
                .ic(style="background-image: url(images/ic-rect-check-send.png)")
              h4 <b>Terkirim</b>
              .text
                p Muatan Anda telah terbongkar di tujuan.
              .total
                span 369
              .link-seemore
                a(href="#") Lihat lebih lanjut
          .col-lg-2
            .box-card.small.send.bg-white.animate.animate--up
              .icon
                .ic(style="background-image: url(images/ic-rect-sent.png)")
              h4 <b>Dikirim</b>
              .text
                p Muatan Anda sedang dalam perjalanan.
              .total
                span 200
              .link-seemore
                a(href="#") Lihat lebih lanjut
          .col-lg-2
            .box-card.small.pending.bg-white.animate.animate--up
              .icon
                .ic(style="background-image: url(images/ic-rect-pending.png)")
              h4 <b>Tertunda</b>
              .text
                p Muatan Anda mengalami keterlambatan.
              .total
                span 4
              .link-seemore
                a(href="#") Lihat lebih lanjut
          .col-lg-6
            .poa__slider.animate.animate--up
              .slider-item
                .box-card.row
                  .col-md-6
                    .icon
                      .ic(style="background-image: url(images/ic-rect-message.png)")
                    h4 <b>Kritik atau Saran?</b>
                    .text
                      p Agros Team sangat terbuka dengan kritik dan saran Anda demi kenyaman Anda.
                    .link-seemore
                      a(href="#") <b>Masukan Kritik atau Saran</b>
                  .col-md-6
                    img(src="images/img-login.png", alt="img")
              .slider-item
                .box-card.row
                  .col-md-6
                    .icon
                      .ic(style="background-image: url(images/ic-rect-message.png)")
                    h4 <b>Kritik atau Saran?</b>
                    .text
                      p Agros Team sangat terbuka dengan kritik dan saran Anda demi kenyaman Anda.
                    .link-seemore
                      a(href="#") <b>Masukan Kritik atau Saran</b>
                  .col-md-6
                    img(src="images/img-login.png", alt="img")

